package com.chinaops.web.agent.service;

import java.util.List;

import com.chinaops.web.agent.dao.UserDaoImpl;
import com.chinaops.web.agent.entity.User;

public class UserService {
	
	private UserDaoImpl userDaoImpl;

	public UserDaoImpl getUserDaoImpl() {
		return userDaoImpl;
	}

	public void setUserDaoImpl(UserDaoImpl userDaoImpl) {
		this.userDaoImpl = userDaoImpl;
	}
	
	/**
	 * Description: 根据用户名获取用户
	 * @Version 1.0 2013-8-21 上午09:28:23崔万哲(cuiwanzhe@china-ops.com) 创建
	 * @param username
	 * @return
	 */
	public List<User> getUserByUsername(String username){
		return this.userDaoImpl.getUserByUsername(username);
	}
}
