//agent上一页
function agentPre(){
	var agent_pageNum = $("#agent_pageNum").html();
	if(agent_pageNum!=1){
		$("#agent_pageNum").html(parseInt(agent_pageNum)-1);
		initagentsAndfils();
	}
}
//agent下一页
function agentNext(){
	var agent_pageNum = $("#agent_pageNum").html();
	var agent_totalPage = $("#agent_totalPage").html();
	if(agent_pageNum!=agent_totalPage){
		$("#agent_pageNum").html(parseInt(agent_pageNum)+1);
		initagentsAndfils();
	}
}
//agent首页
function agentfirst(){
	$("#agent_pageNum").html(1);
	initagentsAndfils();
}
//agent末页
function agentlast(){
	var agent_totalPage = $("#agent_totalPage").html();
	$("#agent_pageNum").html(agent_totalPage);
	initagentsAndfils();
}
