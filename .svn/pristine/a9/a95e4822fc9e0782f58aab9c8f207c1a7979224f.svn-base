package com.chinaops.web.agent.dao;

import java.util.List;

import com.chinaops.web.agent.base.GenericHibernateDao;
import com.chinaops.web.agent.entity.User;

public class UserDaoImpl extends GenericHibernateDao<User>{
	
	public List<User> getUserByUsername(String username){
		return this.find("from User where username=?",new Object[] {username});
	}
}
